  "compilerOptions": {
    "target": "es6", 
    "module": "commonjs", 
    "outDir": "./build", 
    "rootDir": "./src", 
    "sourceMap": true,
    "strict": true, 
    "esModuleInterop": true, 
    "removeComments": true,
    "forceConsistentCasingInFileNames": true, 
    "resolveJsonModule": true,
  }
}
